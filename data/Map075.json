de":401,"indent":0,"parameters":["トレバー"]},{"code":401,"indent":0,"parameters":["「その後、国中の支部に連絡を入れて"]},{"code":401,"indent":0,"parameters":["　見つけ次第報告するよう言ったものの、"]},{"code":401,"indent":0,"parameters":["　今のところ手がかり無し。」"]},{"code":101,"indent":0,"parameters":["Actor2",6,0,2]},{"code":401,"indent":0,"parameters":["トレバー"]},{"code":401,"indent":0,"parameters":["「ただ、ご主人がマジェスタ鉱石を使った"]},{"code":401,"indent":0,"parameters":["　杖の作成をエストタウンの魔術師から"]},{"code":401,"indent":0,"parameters":["　作成するよう頼まれてました。」"]},{"code":101,"indent":0,"parameters":["Actor2",6,0,2]},{"code":401,"indent":0,"parameters":["トレバー"]},{"code":401,"indent":0,"parameters":["「今はその魔術師の行方を探しています。」"]},{"code":101,"indent":0,"parameters":["Actor1",6,0,2]},{"code":401,"indent":0,"parameters":["クラーク"]},{"code":401,"indent":0,"parameters":["「マジェスタ鉱石…"]},{"code":401,"indent":0,"parameters":["  マジェスタ王国原産の強大な魔力の"]},{"code":401,"indent":0,"parameters":["　こもった鉱石の事だな。」"]},{"code":101,"indent":0,"parameters":["Actor3",2,0,2]},{"code":401,"indent":0,"parameters":["商人ギルドオーナー・アルバーノ"]},{"code":401,"indent":0,"parameters":["「マジェスタ鉱石か。"]},{"code":401,"indent":0,"parameters":["　危険な鉱石だから、我々の方じゃ"]},{"code":401,"indent":0,"parameters":["　取り扱う場合はチェックを入れておる。」"]},{"code":101,"indent":0,"parameters":["Actor3",2,0,2]},{"code":401,"indent":0,"parameters":["アルバーノ"]},{"code":401,"indent":0,"parameters":["「じゃが、マジェスタから非正規のルートで"]},{"code":401,"indent":0,"parameters":["　輸入している輩がおるらしい。」"]},{"code":101,"indent":0,"parameters":["Actor1",6,0,2]},{"code":401,"indent":0,"parameters":["クラーク"]},{"code":401,"indent":0,"parameters":["「なるほど…そこで取り扱っている業者が"]},{"code":401,"indent":0,"parameters":["　怪しいな。」"]},{"code":101,"indent":0,"parameters":["Actor3",2,0,2]},{"code":401,"indent":0,"parameters":["アルバーノ"]},{"code":401,"indent":0,"parameters":["「ちょいと時間をくれたら、"]},{"code":401,"indent":0,"parameters":["　各支部に連絡を入れて調べさせよう。」"]},{"code":101,"indent":0,"parameters":["Actor1",6,0,2]},{"code":401,"indent":0,"parameters":["クラーク"]},{"code":401,"indent":0,"parameters":["「ぜひともお願いします。」"]},{"code":101,"indent":0,"parameters":["Actor1",6,0,2]},{"code":401,"indent":0,"parameters":["クラーク"]},{"code":401,"indent":0,"parameters":["「主人に杖作成を依頼した魔術師は"]},{"code":401,"indent":0,"parameters":["　ギルドに加盟していたそうだが、"]},{"code":401,"indent":0,"parameters":["　今はどこにいるのだね？」"]},{"code":101,"indent":0,"parameters":["Actor2",2,0,2]},{"code":401,"indent":0,"parameters":["魔法ギルドオーナー・ローランド"]},{"code":401,"indent":0,"parameters":["「それが、ちょうど数日前から"]},{"code":401,"indent":0,"parameters":["　行方不明になっていましてね。"]},{"code":401,"indent":0,"parameters":["　名前は、ノエルというのですが…。」"]},{"code":101,"indent":0,"parameters":["Actor2",2,0,2]},{"code":401,"indent":0,"parameters":["ローランド"]},{"code":401,"indent":0,"parameters":["「もしかしたらご主人と同じく行方を"]},{"code":401,"indent":0,"parameters":["　くらましたか…」"]},{"code":101,"indent":0,"parameters":["Actor2",2,0,2]},{"code":401,"indent":0,"parameters":["ローランド"]},{"code":401,"indent":0,"parameters":["「あるいは、そのノエルが、"]},{"code":401,"indent":0,"parameters":["　ご主人と何らかのトラブルを"]},{"code":401,"indent":0,"parameters":["　起こしたのかもしれません。」"]},{"code":101,"indent":0,"parameters":["Actor1",6,0,2]},{"code":401,"indent":0,"parameters":["クラーク"]},{"code":401,"indent":0,"parameters":["「なるほど…。」"]},{"code":101,"indent":0,"parameters":["Actor1",6,0,2]},{"code":401,"indent":0,"parameters":["クラーク"]},{"code":401,"indent":0,"parameters":["「マジェスタ鉱石の出どころと、"]},{"code":401,"indent":0,"parameters":["　杖作成を依頼した、ノエルの行方。"]},{"code":401,"indent":0,"parameters":["　これを掴むことが先決のようだな。」"]},{"code":101,"indent":0,"parameters":["Actor2",2,0,2]},{"code":401,"indent":0,"parameters":["ローランド"]},{"code":401,"indent":0,"parameters":["「わかりました。"]},{"code":401,"indent":0,"parameters":["　ノエルの居場所について、"]},{"code":401,"indent":0,"parameters":["　各ギルド支部に詳しく聞いてみます。」"]},{"code":101,"indent":0,"parameters":["Actor1",6,0,2]},{"code":401,"indent":0,"parameters":["クラーク"]},{"code":401,"indent":0,"parameters":["「苦労をかける。すまない。」"]},{"code":101,"indent":0,"parameters":["Actor2",2,0,2]},{"code":401,"indent":0,"parameters":["ローランド"]},{"co